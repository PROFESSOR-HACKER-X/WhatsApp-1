{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ],
  "functions": {
    "pages/api/bot.js": {
      "maxDuration": 60
    },
    "pages/api/status.js": {
      "maxDuration": 30
    }
  }
}